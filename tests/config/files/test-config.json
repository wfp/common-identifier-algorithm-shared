        {
          "op": "field_type",
          "value": "str"
        },
        {
          "op": "same_value_for_all_rows"
        },
        {
          "message": "must be SYP (Syrian Pounds)",
          "op": "regex_match",
          "value": "SYP"
        }
      ],
      "end": [
        {
          "message": "must be in the format of yyyyMMdd, e.g. 20240301",
          "op": "regex_match",
          "value": "((?:19|20)\\d{2}(?:0[1-9]|1[012])(?:0[1-9]|[12]\\d|3[01]))"
        },
        {
          "message": "must be within a year of Start",
          "op": "date_field_diff",
          "target": "start",
          "value": "-12M"
        }
      ],
      "national_id": [
        {
          "op": "field_type",
          "value": "str"
        },
        {
